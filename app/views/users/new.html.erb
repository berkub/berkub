<%= simple_form_for @user do |f| %>
	<h1><%= t("users.new.title") %></h1>
	<fieldset class="inline">
		<%= f.input :first_name, autofocus: true %>
		<%= f.input :last_name %>
	</fieldset>
	<%= f.input :email %>
	<%= f.input :password %>
	<div class="actions">
		<div class="beware"><%= t("users.create.beware") %></div>
		<%= f.submit t("users.") %>
	</div>
<% end %>